{
  "name": "next-express-trpc",
  "version": "0.1.0",
  "description": "This TypeScript template provides a robust and well-structured foundation for building modern web applications. It leverages Next.js for frontend development, Express.js for backend APIs, and integrates Socket.IO for real-time communication. The template incorporates tRPC for type-safe data fetching, includes examples of SSR, SSG, and dynamic pages, and features examples of secure cookie management. With a focus on best practices and a modular design, this template streamlines development and enhances code quality, making it an ideal starting point for various web projects.",
  "private": true,
  "scripts": {
    "lint": "next lint",
    "schema:local": "npx wrangler d1 execute prod-d1-demo --local --file=./cloudflare/database/schema.sql",
    "schema:cloud": "npx wrangler d1 execute prod-d1-demo --remote --file=./cloudflare/database/schema.sql",
    "dev": "cross-env  NODE_ENV=development next dev --turbopack -p 5438",
    "dev:sqlite": "schema:local && cross-env  NODE_ENV=development next dev --turbopack -p 5438",
    "build": "next build --debug",
    "start": "cross-env NODE_ENV=production next start -p 5438",
    "node:dev": "cross-env  NODE_ENV=development nodemon --watch './server/**/*.ts' --exec 'ts-node' ./server.ts",
    "node:build": "next build --debug && cross-env NODE_ENV=production ts-node ./server.ts",
    "node:start": "cross-env  NODE_ENV=production ts-node ./server.ts",
    "worker:build": "opennextjs-cloudflare",
    "worker:dev": "wrangler dev --port 5438",
    "worker:preview": "npm run worker:build && npm run worker:dev",
    "worker:preview_now": "npm run worker:dev",
    "worker:deploy": "npm run worker:build && wrangler deploy",
    "worker:deploy_now": "wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
